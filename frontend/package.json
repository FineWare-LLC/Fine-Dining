{
  "name": "frontend",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "npm run seed && next dev",
    "build": "npm run seed && next build",
    "build:plugins": "node scripts/build-plugins.mjs",
    "build:gpu": "cross-env USE_GPU=1 next build",
    "start": "next start",
    "lint": "next lint",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:report": "eslint . --ext .js,.jsx,.ts,.tsx --format html --output-file eslint-report.html",
    "test": "node --test src/tests/unit/*test.js src/tests/unit/*test.mjs src/tests/solver/*test.js src/tests/gpu/*test.mjs",
    "test:unit": "node --test src/tests/unit/*test.js src/tests/unit/*test.mjs",
    "test:solver": "node --test src/tests/solver/*test.js",
    "test:gpu": "node --test src/tests/gpu/*test.mjs",
    "test:watch": "node --test --watch src/tests/unit/*test.js src/tests/unit/*test.mjs src/tests/solver/*test.js src/tests/gpu/*test.mjs",
    "test:coverage": "node --test --experimental-test-coverage src/tests/unit/*test.js src/tests/unit/*test.mjs src/tests/solver/*test.js src/tests/gpu/*test.mjs",
    "test:playwright": "node scripts/run-playwright.mjs",
    "test:components": "cross-env NODE_ENV=test-ct playwright test --config=playwright-ct.config.js",
    "test:components:headed": "cross-env NODE_ENV=test-ct playwright test --config=playwright-ct.config.js --headed",
    "test:components:debug": "cross-env NODE_ENV=test-ct playwright test --config=playwright-ct.config.js --debug",
    "test:components:ui": "cross-env NODE_ENV=test-ct playwright test --config=playwright-ct.config.js --ui",
    "test:e2e": "playwright test --config=playwright.config.js",
    "test:e2e:headed": "playwright test --config=playwright.config.js --headed",
    "test:e2e:debug": "playwright test --config=playwright.config.js --debug",
    "test:e2e:ui": "playwright test --config=playwright.config.js --ui",
    "test:e2e:chromium": "playwright test --config=playwright.config.js --project=chromium",
    "test:e2e:firefox": "playwright test --config=playwright.config.js --project=firefox",
    "test:e2e:webkit": "playwright test --config=playwright.config.js --project=webkit",
    "test:e2e:mobile": "playwright test --config=playwright.config.js --project=\"Mobile Chrome\"",
    "test:all": "npm run test && npm run test:playwright",
    "test:report": "playwright show-report",
    "test:report:components": "playwright show-report playwright-report/component-tests",
    "seed": "node ../highs-pipeline/seed_with_polyfill.mjs",
    "dev:memory": "node scripts/dev-memory.mjs",
    "codegen": "graphql-codegen --config codegen.yml",
    "benchmark:gpu": "node src/gpu/benchmark.mjs 1024"
  },
  "dependencies": {
    "@apollo/client": "^4.0.7",
    "@apollo/server": "^5.0.0",
    "@as-integrations/next": "^4.0.0",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.14.0",
    "@faker-js/faker": "^9.6.0",
    "@headlessui/react": "^1.7.18",
    "@mui/icons-material": "^5.15.10",
    "@mui/material": "^5.15.10",
    "@playwright/experimental-ct-react": "^1.52.0",
    "@tanstack/react-query": "^5.38.0",
    "bcrypt": "^5.1.1",
    "browser-image-compression": "^2.0.0",
    "dotenv": "^16.4.7",
    "framer-motion": "^12.7.3",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "highs-addon": "^0.9.4",
    "immer": "^10.1.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.563.0",
    "mongoose": "^8.14.2",
    "mongoose-encryption": "^2.1.2",
    "next": "^15.3.2",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-window": "^1.8.11",
    "react-window-infinite-loader": "^1.0.10",
    "winston": "^3.10.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/plugin-transform-class-properties": "^7.25.9",
    "@babel/plugin-transform-object-rest-spread": "^7.25.9",
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^9.30.1",
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/client-preset": "^4.8.0",
    "@playwright/test": "^1.52.0",
    "@tailwindcss/postcss": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.26.0",
    "eslint-config-next": "15.3.2",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unicorn": "^59.0.1",
    "lighthouse": "^10.4.0",
    "mongodb-memory-server": "^8.14.0",
    "postcss": "^8.5.3",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.6"
  },
  "optionalDependencies": {
    "opencl": "^1.0.0"
  }
}
